{
  "manifest_version": 3,
  "name": "CyberTab",
  "version": "1.0.5",
  "description": "Cyberpunk new tab - no login, no ads. Add favorites as movable app icons.",
  "chrome_url_overrides": { "newtab": "newtab.html" },
  "permissions": ["storage","bookmarks"],
  "icons": { "16": "icons/icon48.png", "48": "icons/icon48.png", "128": "icons/icon128.png" },
  "action": {
    "default_icon": {
      "16": "icons/icon48.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "CyberTab"
  },
  "default_locale": "en"
}